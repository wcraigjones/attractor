digraph branch_ctx {
    graph [goal="Test parallel branch context"]

    start   [shape=Mdiamond]
    fork    [shape=component]
    branch1 [shape=box, prompt="Branch 1"]
    branch2 [shape=box, prompt="Branch 2"]
    join    [shape=tripleoctagon]
    done    [shape=Msquare]

    start   -> fork
    fork    -> branch1
    fork    -> branch2
    branch1 -> join
    branch2 -> join
    join    -> done
}
